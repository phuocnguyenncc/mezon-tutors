{
  "name": "@mezon-tutors/db",
  "version": "0.0.0",
  "description": "",
  "author": "timour",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn prisma:gen && tsc",
    "clean": "npx rimraf dist",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "format": "biome check --write .",
    "prisma:db:push": "dotenv -e .env -- npx prisma db push",
    "prisma:reset": "dotenv -e .env -- npx prisma migrate reset",
    "prisma:fmt": "dotenv -e .env -- npx prisma format",
    "prisma:gen": "dotenv -e .env -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "seed": "dotenv -e .env -- tsx prisma/seed",
    "prisma:migrate:data": "scaffdog generate"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@faker-js/faker": "^10.2.0",
    "@quramy/prisma-fabbrica": "^2.2.1",
    "@types/node": "^24.3.0",
    "@types/uuid": "^9.0.5",
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.2.1",
    "rimraf": "^5.0.5",
    "scaffdog": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3"
  }
}
